{
    "include": ["src/**/*"],
    "compilerOptions": {
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable",
        ],
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "skipLibCheck": true,
        "noUncheckedIndexedAccess": true,
        "types": ["vite/client"]
    }
}
